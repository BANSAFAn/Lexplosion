<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:authviewmodels="clr-namespace:Lexplosion.WPF.NewInterface.ViewModels.Authorization"
    xmlns:authviews="clr-namespace:Lexplosion.WPF.NewInterface.Views.Pages.Authorization"
    xmlns:viewmodels="clr-namespace:Lexplosion.WPF.NewInterface.ViewModels">

    <viewmodels:MainViewModel x:Key="MainViewModel" />

    <!--  Authorization  -->
    <DataTemplate DataType="{x:Type authviewmodels:AuthorizationMenuViewModel}">
        <authviews:AuthorizationMenuView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type authviewmodels:NightWorldAuthFormViewModel}">
        <authviews:NightWorldAuthFormView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type authviewmodels:PasswordResetViewModel}">
        <authviews:PasswordResetView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type authviewmodels:DigitCodeConfirmationVIewModel}">
        <authviews:DigitCodeConfirmationVIew />
    </DataTemplate>
</ResourceDictionary>