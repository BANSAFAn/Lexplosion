<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="ActivityColor">#167FFC</Color>
    <Color x:Key="MyLovelyColor">#ff7601</Color>
    <SolidColorBrush x:Key="BrandSolidColorBrush" Color="{StaticResource ActivityColor}" />

    <Color x:Key="PrimaryColor">#191919</Color>
    <Color x:Key="PrimaryColor1">#131313</Color>
    <Color x:Key="LightPrimaryColor">#f8f8f8</Color>

    <Color x:Key="HeaderColor">#151719</Color>
    <Color x:Key="SepColor">#212326</Color>
    <Color x:Key="TextBoxColor">#1f1f1f</Color>

    <SolidColorBrush x:Key="PrimarySolidColorBrush" Color="{StaticResource PrimaryColor}" />
    <SolidColorBrush x:Key="HeaderSolidColorBrush" Color="{StaticResource HeaderColor}" />
    <SolidColorBrush x:Key="SepSolidColorBrush" Color="{StaticResource SepColor}" />
    <SolidColorBrush x:Key="TextBoxSolidColorBrush" Color="{StaticResource TextBoxColor}" />
</ResourceDictionary>